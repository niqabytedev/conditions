//Example of if else statement
//By niqabytedev

#include <iostream>
using namespace std;
int main()
{
int x, y, z, enk, enb;
	cout << "3 sayi giriniz" << endl;
	cin >> x >> y >> z;

	if (x > y && x > z) {
		enb = x;
		enk = (y < z) ? y : z;
	}
	else if (y > z && y > x) {
		enb = y;
		enk = (x < z) ? x : z;
	}
	else {
		enb = z;
		enk = (x < y) ? x : y;
	}
	cout << "en buyuk sayi: " << enb << endl;
	cout << "en kucuk sayi: " << enk << endl;
}
